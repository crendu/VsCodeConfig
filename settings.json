    "files.encoding": "utf8",
    "files.exclude": {
        "**/*.acx": true,
        "**/*.csproj": true,
        // "**/*.dat": true,
        "**/*.dsp": true,
        // "**/*.git": true,
        "**/*.hhc": true,
        "**/*.htm": true,
        "**/*.md": true,
        "**/*.o": true,
        "**/*.olf": true,
        "**/*.pro": true,
        // "**/*.project": true,
        // "**/*.settings": false,
        "**/*.sgx": true,
        "**/*.sln": true,
        "**/*.updt": true,
        "**/*.vcproj": true,
        "**/*.vcxproj*": true,
        "**/*.vdproj": true,
        // "**/bin/": true,
        // "**/build/": true,
        "**/Intermediates*": true,
        // "**/javatest/": true,
        "**/JenkinsReferences*": true,
        // "**/shared/": true,
        "**/Software*": true,
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
