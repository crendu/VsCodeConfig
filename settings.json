     * FILES PARAMETERS *
     ********************/
    "files.associations": {
        "*.cfg": "libconfig",
        "*.gcp": "makefile",
        "*.qss": "css",
        "*.rc": "rc-script",
        "*.rsc": "properties",
        "*.out": "log",
    },
