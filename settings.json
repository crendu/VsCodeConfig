{
    /***************
     * ENVIRONMENT *
     ***************/
    "diffEditor.maxComputationTime": 0,
    "diffEditor.hideUnchangedRegions.enabled": true,

    "editor.codeActionsOnSave": {
        "source.organizeImports": "never"
    },
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.detectIndentation": false,
    "editor.fontFamily": "Fira Code, Cascadia Code PL, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.guides.bracketPairs": true,
    "editor.insertSpaces": true,
    "editor.largeFileOptimizations": false,
    "editor.minimap.showSlider": "always",
    "editor.multiCursorLimit": 100000,
    "editor.padding.bottom": 200,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": false
    },
    "editor.quickSuggestionsDelay": 10,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        80,
        {
            "column": 120,
            "color": "#ff4081"
        },
        {
            "column": 160,
            "color": "#ff0000"
        },
    ],
    "editor.scrollBeyondLastLine": false,
    "editor.showDeprecated": true,
    "editor.showUnused": true,
    "editor.stickyScroll.defaultModel": "foldingProviderModel",
    "editor.stickyScroll.enabled": true,
    "editor.stickyScroll.scrollWithEditor": false,
    "editor.suggestSelection": "first",
    "editor.tabSize": 4,
    "editor.unicodeHighlight.ambiguousCharacters": false,
    "editor.unicodeHighlight.invisibleCharacters": true,
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "explorer.confirmDelete": false,

    "security.allowedUNCHosts": [ "nassvg" ],
    "security.workspace.trust.enabled": false,
    "telemetry.telemetryLevel": "off",
    "update.mode": "default",
    "window.zoomLevel": -1,

    "workbench.activityBar.location": "top",
    "workbench.colorCustomizations": {
        "editorUnnecessaryCode.border": "#ff4081",
        "minimapSlider.activeBackground": "#ffff002b",
        "minimapSlider.background": "#ffff002b",
        "minimapSlider.hoverBackground": "#ffff002b",
        "scrollbarSlider.activeBackground": "#ffff002b",
        "scrollbarSlider.background": "#ffff002b",
        "scrollbarSlider.hoverBackground": "#ffff002b",
        "tab.activeBackground": "#0c3953",
        "tab.activeBorder": "#0a08b2",
        "tab.activeBorderTop": "#0c3953",
        "tab.border": "#0c3953",
        "tab.hoverBackground": "#094c72",
    },
    "workbench.colorTheme": "SynthWave '84 Blues",
    "workbench.editor.enablePreview": true,
    "workbench.editor.enablePreviewFromCodeNavigation": true,
    "workbench.editor.enablePreviewFromQuickOpen": true,
    "workbench.editor.splitInGroupLayout": "horizontal",
    "workbench.editorLargeFileConfirmation": 2048,
    "workbench.layoutControl.enabled": false,
    "workbench.startupEditor": "none",


    /********************
     * FILES PARAMETERS *
     ********************/
    "files.associations": {
        "*.cfg": "libconfig",
        "*.gcp": "makefile",
        "*.qss": "css",
        "*.rc": "rc-script",
        "*.rsc": "properties",
        "*.out": "log",
    },
    "files.autoGuessEncoding": true,   // To resolve pb with UTF8 VS Windows1252 encoding.
    "files.encoding": "utf8",
    "files.exclude": {
        "**/*.acx": true,
        "**/*.csproj": true,
        // "**/*.dat": true,
        "**/*.dsp": true,
        // "**/*.git": true,
        "**/*.hhc": true,
        "**/*.htm": true,
        "**/*.md": true,
        "**/*.o": true,
        "**/*.olf": true,
        "**/*.pro": true,
        // "**/*.project": true,
        // "**/*.settings": false,
        "**/*.sgx": true,
        "**/*.sln": true,
        "**/*.updt": true,
        "**/*.vcproj": true,
        "**/*.vcxproj*": true,
        "**/*.vdproj": true,
        // "**/bin/": true,
        // "**/build/": true,
        "**/Intermediates*": true,
        // "**/javatest/": true,
        "**/JenkinsReferences*": true,
        // "**/shared/": true,
        "**/Software*": true,
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.watcherExclude": {   // Seems to avoid the ipch flood
        "Intermediates/**": true,
        "Software/**": true
    },
    "search.exclude": {
        "**.css": true,
        "**.err": true,
        "**.html": true,
        "**.js": true,
        // "**.log": true,
        "**.map": true,
        "**.php": true,
        "**.xsl": true,
        "**.yaml": true,
        "**/doc*/": true,
        "**/Documentation/": true,
        // "**/target/": true,
        // "**/templates/": true,
        "**/*w10-x64-vc14*": true,
        "**/*w7-x86-vc10*": true,
    },
    "search.followSymlinks": false, // Seems to avoid High CPU usage

    "[cmd]": { "files.encoding": "windows1252" },
    "[cpp]": { "files.encoding": "windows1252" },
    "[h]": { "files.encoding": "windows1252" },



    /******************
     * COMMAND PROMPT *
     ******************/
