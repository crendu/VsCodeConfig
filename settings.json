    // Log File Highlighter
        "logFileHighlighter.customPatterns": [
            {
                "foreground": "#ff0000",
                "overviewColor": "#ff0000",
                "overviewRulerLane": "Left",
                "pattern": ".*ERREUR.*|\\[ERROR]|(^.* KO.*|(fatal error| error) \\w+)",
            },
            {
                "foreground": "#22aa22",
                "pattern": "^.* OK.*",
            },
            {
                "foreground": "#ffa500",
                "pattern": ".*AVERTISSEMENT.*|\\[DEBUG]|(^.*warning : |warning \\w+)",
            },
            {
                "fontWeight": "bold",
                "foreground": "#fa8072",
                "pattern": "[0-9]{4}/[0-9]{2}/[0-9]{2}-[0-9]{2}:[0-9]{2}:[0-9]{2}.[0-9]{3}",
            },
            {
                "fontStyle": "italic",
                "foreground": "#fa8072",
                "pattern": "(d|D):.*?\\.(bmx|cc|chm|dat|dll|decl|enum|err|FS|info|inl|iv|lib|log|mod|msg|obj|pl|pm|ref|reg|tex|VS)",
            },
            {
                "fontStyle": "italic",
                "foreground": "#9d9dff",
                "pattern": "(d|D):.*?\\.(cmd|cpp|cfg|bmp|fdp|h|ico|png|txt|xml)",
            },
            {
                "foreground": "#0080ff",
                "pattern": "\\[.*]",
            },
        ],

