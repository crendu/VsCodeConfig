{
    /***************
     * ENVIRONMENT *
     ***************/
    "diffEditor.maxComputationTime": 0,
    "diffEditor.hideUnchangedRegions.enabled": true,

    "editor.codeActionsOnSave": {
        "source.organizeImports": "never"
    },
    "editor.cursorBlinking": "phase",
